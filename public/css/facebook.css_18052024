* {
    margin: 0;
    padding: 0;
    text-decoration: none;
    list-style: none;
    outline: none;
}

* {
    font-family: "Source Sans Pro", -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
}

body {
    font-size: 12px;
    color: #3b3b3b;
    word-spacing: normal;
    background: #f3f1f1;
}

a {
    text-decoration: none;
    color: #555;
}

a:visited {
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
    color: #3974aa;
    text-decoration: none;
}

a:active {
    text-decoration: none;
}

input,
select,
button {
    font-size: 12px;
}

form em {
    color: #F00 !important;
}

h1 {
    font-size: 12px;
    font-weight: normal;
}

h2 {
    font-size: 12px;
    text-transform: uppercase;
    font-weight: normal;
}

h3 {
    font-weight: normal;
}

h4 {
    font-weight: normal;
}

h5 {
    font-weight: normal;
}

h6 {
    font-size: 12px;
    font-weight: bold;
    color: #1d1d1d;
    background: url(../../images/black-arrow.jpg) 0 center no-repeat;
    padding: 0 0 1px 20px;
    clear: both;
    display: block;
}

p {
    font-weight: normal;
}

img {
    border: 0;
}

a img {
    border: 0;
    margin: 0;
    padding: 0;
}

.clear {
    clear: both;
}

.float-right {
    float: right !important;
}

.float-left {
    float: left !important;
}

.boder0-left {
    border-left: none !important;
}

.boder0-right {
    border-right: none !important;
}

.red-t {
    color: #F00;
    font-size: 12px;
}

.red-t2 {
    color: #F00;
    font-size: 10px;
    font-weight: bold;
}

.help-desk-view-tick-h6 {
    padding: 1px 0 0 18px;
    display: block;
    clear: both;
    margin: 26px 0 8px 0;
}

.sr-icon-deactive {
    filter: url(filters.svg#grayscale);
    /* Firefox 3.5+ */
    filter: gray;
    /* IE6-9 */
    -webkit-filter: grayscale(1);
    /* Google Chrome, Safari 6+ & Opera 15+ */
}

.wrapper {
