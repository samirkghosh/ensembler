/*  ====================================================== Call Status bar */

.callstatus-panel {
    float: left;
    border: #d4d4d4 1px solid;
    text-align: left;
    color: #4a4a4a;
    width: 262px;
    margin-bottom: 20px;
    overflow: hidden;
}

.callstatus-panel h2 {
    background: #222;
    color: #fff;
    padding: 10px 10px;
}

.callstatus-panel ul {
    list-style: none;
}

.callstatus-panel ul li {
    background: #f3f1f1;
    border-bottom: #cecece 1px solid;
    float: left;
    width: 100%;
}

.callstatus-panel ul li span {
    font-size: 29px;
    width: 55px;
    background: #e3e3e3;
    border-left: #cecece 1px solid;
    padding: 10px 10px;
    float: right;
    text-align: center;
}

.callstatus-panel ul li:last-child {
    border-bottom: none !important;
}

.callstatus-panel ul li p {
    display: inline-block;
    padding: 21px 0 0 15px;
}

.callstatus-panel .regular-link {
    background: #f5f5f5;
    border-bottom: #cecece 1px solid;
    display: block;
    line-height: 38px;
    height: 38px;
    width: 268px;
}

.callstatus-panel .regular-link a:nth-child(1) {
    border: none;
    padding: 0 8px 0 7px;
}

.callstatus-panel .regular-link a {
    float: left;
    padding: 0 7px 0 7px;
    font-size: 11px;
    border-left: #cecece 1px solid;
    cursor: pointer;
}

.callstatus-panel .regular-link a.active,
.callstatus-panel .regular-link a:hover {
    border-top: #f29d3f 3px solid;
    height: 36px;
    line-height: 36px;
    background: #f6f6f6;
    /* Old browsers */
    background: -moz-linear-gradient(top, #f6f6f6 0%, #d3d3d3 100%);
    /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #f6f6f6), color-stop(100%, #d3d3d3));
    /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #f6f6f6 0%, #d3d3d3 100%);
    /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #f6f6f6 0%, #d3d3d3 100%);
    /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #f6f6f6 0%, #d3d3d3 100%);
    /* IE10+ */
    background: linear-gradient(to bottom, #f6f6f6 0%, #d3d3d3 100%);
    /* W3C */
    filter: progid: DXImageTransform.Microsoft.gradient(startColorstr='#f6f6f6', endColorstr='#d3d3d3', GradientType=0);
    /* IE6-9 */
}

#dt_range_dis {
    padding-left: 15px;
    color: #f29d3f;
}

.callstatus-form {
    padding: 10px;
    float: left;
}

.callstatus-form .istyle1 {
    width: 99px;
    /*height:26px;*/
    float: left;
